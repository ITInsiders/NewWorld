@using NW.PL.Models;
@{
    Layout = ViewBag.Page.LayoutHref;
    Identity Identity = new Identity();
    ViewBag.Identity = Identity;
}
<div class="Option">
    <a class="Back" href="/Home/Index"></a>
    <form class="SearchBlock" action="javascript:SearchBlocks();">
        <input type="search" placeholder="Поиск" class="Search" id="Search" list="SearchList" />
        <datalist id="SearchList"></datalist>
        <button class="SearchButton" id="SearchButton"></button>
    </form>
    <div class="Filter"></div>


    <div class="Blocks" id="SearchResult">
    </div>
</div>
<div class="map" id="map">
</div>

<div class="Templates" id="Templates" style="display: none">
    <a class="Block Empty">
        <div class="Photo"><div class="Ratings"></div></div>
        <div class="Data" data-position="">
            <div class="Title Name Place"></div>
            <div class="Title Info Category"></div>
            <div class="Title Info Adress"></div>
            <div class="Title Info Time"></div>
            <div class="Title Distance"></div>
        </div>
    </a>
    <div class="Block Null">
        <span> По Вашему запросу ничего не найдено! </span>
    </div>
</div>